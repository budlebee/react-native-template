{
  "name": "kundera",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "and-stag": "react-native run-android --variant=stagingdebug",
    "and-stag-release": "react-native run-android --variant=stagingrelease",
    "and-dev": "react-native run-android --variant=developmentdebug",
    "and-dev-release": "react-native run-android --variant=developmentrelease",
    "and-prod": "react-native run-android --variant=productiondebug",
    "and-prod-release": "react-native run-android --variant=productionrelease",
    "ios": "react-native run-ios",
    "ios-dev": "react-native run-ios --scheme kunderaDev",
    "ios-prod": "react-native run-ios --scheme kunderaProd",
    "ios-stag": "react-native run-ios --scheme kunderaStag",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.14",
    "@react-native-firebase/app": "^13.0.1",
    "@react-native-firebase/messaging": "^13.0.1",
    "axios": "^0.24.0",
    "react": "17.0.2",
    "react-native": "0.66.3",
    "react-native-config": "^1.4.5",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "@react-native-community/eslint-config": "^3.0.1",
    "babel-jest": "^27.4.2",
    "eslint": "^8.3.0",
    "jest": "^27.4.2",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
